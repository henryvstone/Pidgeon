var moveDown = keyboard_check(ord("S"));
var moveLeft = keyboard_check(ord("A"));
var moveRight = keyboard_check(ord("D"));

var lr = 0;
if(moveLeft){lr -= 1;}
if(moveRight){lr += 1;}
var ud = 0;
if(moveDown){ud += 1;}
if(moveUp){ud -= 1;}

if(lr != 0 or ud != 0){
    var sp = delta_time / 1000000 * moveSpeed / sqrt(lr * lr + ud * ud);
    
    px += lr * sp;
    py += ud * sp;
}

